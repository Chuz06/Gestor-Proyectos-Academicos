@model Gestor_Proyectos_Academicos.Models.Rol

@{
    ViewData["Title"] = "Detalles del Rol";
}

<h2>Detalles del Rol</h2>

<div>
    <dl class="row">
        <dt class="col-sm-2">Id</dt>
        <dd class="col-sm-10">@Model.IdRol</dd>

        <dt class="col-sm-2">Nombre</dt>
        <dd class="col-sm-10">@Model.NombreRol</dd>
    </dl>
</div>

<p>
    <a asp-action="Edit" asp-route-id="@Model.IdRol" class="btn btn-primary">Editar</a>
    <a asp-action="Index" class="btn btn-secondary">Volver</a>
</p>
